<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ifpb.atividadecolaborativa_pdm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="ifpb.ads.pdm.atividadecolaborativa.telas.Cadastro"></activity>
        <activity android:name="ifpb.ads.pdm.atividadecolaborativa.telas.Login"></activity>
        <activity android:name="ifpb.ads.pdm.atividadecolaborativa.telas.Principal"></activity>
        <activity android:name="ifpb.ads.pdm.atividadecolaborativa.telas.ViewNew"></activity>
        <activity android:name="ifpb.ads.pdm.atividadecolaborativa.telas.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="ifpb.ads.pdm.atividadecolaborativa.servicos.ServiceRequest"/>
        <receiver android:name="ifpb.ads.pdm.atividadecolaborativa.servicos.ReiceverInitService"/>
    </application>

</manifest>